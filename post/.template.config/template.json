{
    "$schema": "http://json.schemastore.org/template",
    "author": "Brady Gaster",
    "classifications": [ "post", "blog" ],
    "identity": "downr.post",
    "name": "downr blog post",
    "shortName": "post",
    "symbols":{
      "lastModified": {
        "type": "generated",
        "generator": "now",
        "parameters": {
          "format": "yyyy-MM-dd hh:mm:ss"
        },
        "replaces":"2020-10-31 00:00:00"
      },
      "pubDate": {
        "type": "generated",
        "generator": "now",
        "parameters": {
          "format": "yyyy-MM-dd hh:mm:ss"
        },
        "replaces":"2020-10-31 00:00:00"
      },
      "title": {
        "type": "parameter",
        "description": "The title of your blog post",
        "defaultValue": "My new downr blog post",
        "replaces": "MY-TITLE"
      },
      "slug": {
        "type": "parameter",
        "description": "A URL-friendly slug that will make your post easy to find",
        "defaultValue": "my-new-downr-blog-post",
        "replaces": "MY-SLUG"
      },
      "author": {
        "type": "parameter",
        "description": "The name you wish to be recognized by on the blog",
        "defaultValue": "author-name",
        "replaces": "MY-NAME"
      },
      "categories": {
        "type": "parameter",
        "description": "A comma-separated list of categories in which your blog will appear",
        "defaultValue": "blog,post,article",
        "replaces": "MY-CATEGORIES"
      },
      "description": {
        "type": "parameter",
        "description": "A description of your site. This will be used in the site RSS feed to introduce your post.",
        "defaultValue": "This is a description of the post.",
        "replaces": "MY-DESCRIPTION"
      }
    }
  }